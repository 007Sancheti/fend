  "tests": [
    {
      "description": "There is an image on the page",
      "definition": {
        "nodes": "img",
        "exists": true
      }
    },
    {
      "description": "The image has the right source",
      "definition": {
        "nodes": "img",
        "attribute": "src",
        "hasSubstring": "^redwoods.jpg$"
      }
    },
    {
      "description": "The image has an alt attribute",
      "definition": {
        "nodes": "img",
        "attr": "alt",
        "hasSubstring": ".+"
      }
    },
    {
      "description": "There is paragraph element after the image",
      "definition": {
        "nodes": "img + p",
        "exists": true
      }
    },
    {
      "description": "The paragraph contains the right link",
      "definition": {
        "nodes": "img + p > a",
        "attr": "href",
        "hasSubstring": "https://en.wikipedia.org/wiki/File:Stout_Memorial_Grove_in_Jedediah_Smith_Redwoods_State_Park_in_2011_(22).JPG"
      }
    },
    {
      "description": "The paragraph attributes the photographer",
      "definition": {
        "nodes": "img + p",
        "get": "innerHTML",
        "hasSubstring": "Stout Memorial Grove in Jedediah Smith Redwoods State Park in 2011"
      }
    }
  ]
